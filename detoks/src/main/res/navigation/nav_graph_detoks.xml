<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    android:label="DeToks"
    app:startDestination="@id/TestFragment">

    <fragment
        android:id="@+id/detoksFragment"
        android:name="nl.tudelft.trustchain.detoks.DeToksFragment"
        android:label="DeToks">
    </fragment>

    <fragment
        android:id="@+id/TestFragment"
        android:name="nl.tudelft.trustchain.detoks.TestFragment"
        android:label="TestFragment">
        <action
            android:id="@+id/action_go_to_testFreq"
            app:destination="@id/TransactionFrequencyTestFragment" />
    </fragment>

    <fragment
        android:id="@+id/exampleOverlayFragment"
        android:name="nl.tudelft.trustchain.detoks.ExampleOverlayFragment"
        android:label="ExampleOverlay">

	  <action
            android:id="@+id/action_go_to_testEnvironment"
            app:destination="@id/TransactionFrequencyTestFragment" />
    </fragment>

    <fragment
        android:id="@+id/TransactionFrequencyTestFragment"
        android:name="nl.tudelft.trustchain.detoks.TransactionFrequencyTestFragment"
        android:label="TransactionFrequencyTest">

        <action
            android:id="@+id/action_switch_environment"
            app:destination="@id/exampleOverlayFragment" />
    </fragment>

    <fragment
        android:id="@+id/singlePeerFragment"
        android:name="nl.tudelft.trustchain.detoks.SinglePeerFragment"
        android:label="SinglePeer">
    </fragment>



</navigation>
